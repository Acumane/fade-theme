                "variable.scss",
                "variable.other.less"
            ],
            "settings": {
                "foreground": "#b8d7a3"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#989898"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#569cd6" // default
            }
        },
        {
            "scope": "keyword.operator",
            "settings": {
