        {
            "scope": "token.info-token",
            "settings": {
                "foreground": "#6796e6" // default
            }
        },
        {
            "scope": "token.warn-token",
            "settings": {
                "foreground": "#cd9731" // default
            }
        },
        {
            "scope": "token.error-token",
            "settings": {
                "foreground": "#f44747" // default
            }
        },
        {
            "scope": "token.debug-token",
            "settings": {
                "foreground": "#b267e6" // default
            }
        }
