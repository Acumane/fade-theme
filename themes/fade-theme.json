        },
        {
            "scope": "storage.modifier",
            "settings": {
                "foreground": "#b37d7d"
            }
        },
        {
            "scope": "string",
            "settings": {
                "foreground": "#9696ff"
            }
        },
        {
            "scope": "string.tag",
            "settings": {
                "foreground": "#ce9178" // default
            }
        },
        {
            "scope": "string.value",
            "settings": {
                "foreground": "#ce9178" // default
            }
        },
        {
            "scope": "string.regexp",
            "settings": {
                "foreground": "#d16969" // default
            }
        },
        {
            "name": "JavaScript string interpolation ${}",
            "scope": [
                "punctuation.definition.template-expression.begin.js",
                "punctuation.definition.template-expression.begin.ts",
