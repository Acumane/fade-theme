{
    "name": "Fade",
    "type": "dark",
    "colors": {
        "editor.background": "#111012",
        "editor.foreground": "#d0d0d0",
        "editor.findMatchBackground": "#264f78",
        "editor.findMatchHighlightBackground": "#782525",
        "editor.hoverHighlightBackground": "#17352f",
        "editor.rangeHighlightBackground": "#000f1a",
        "editorBracketMatch.background": "#264f78",
        "editorBracketMatch.border": "#3d80c2",
        "editorLineNumber.foreground": "#909090",
        "editorRuler.foreground": "#202020",
        "editorWidget.background": "#1e1e20",
        "peekViewEditor.background": "#000f1a",
        "statusBar.background": "#2e3e5b",
        "statusBar.border": "#5673b5",
        "statusBarItem.remoteBackground": "#2f77cc",
        "statusBar.noFolderBackground": "#21567a",

        "notebook.cellEditorBackground": "#111012",
		"notebook.outputContainerBackgroundColor": "#080808",
		"notebook.outputContainerBorderColor": "#252526",

        "activityBarBadge.background": "#007acc",           // default
        "editor.inactiveSelectionBackground": "#3a3d41",    // default
        "editor.selectionHighlightBackground": "#add6ff26", // default
        "editorIndentGuide.background": "#404040",          // default
        "list.dropBackground": "#383b3d",                   // default
        "sideBarTitle.foreground": "#bbbbbb"                // default
    },
    "tokenColors": [
        {
            "settings": {
                "foreground": "#d0d0d0ff",
                "background": "#101010ff"
            }
        },
        {
            "scope": "emphasis",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "strong",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "header",
            "settings": {
                "foreground": "#000080"
            }
        },
        {
            "scope": "comment",
            "settings": {
                "foreground": "#505050"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#d69d85"
            }
        },
        {
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#f8dc5c"
            }
        },
        {
            "scope": "constant.regexp",
            "settings": {
                "foreground": "#646695" // default
            }
        },
        {
